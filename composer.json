{
    "name": "abc-da-construcao/autorizacao-package-client",
    "type": "package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AbcDaConstrucao\\AutorizacaoCliente\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Talles Gazel",
            "email": "talles.gazel@abcdacontrucao.com.br"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/auth": "^5.2|^6|^7|^8",
        "illuminate/contracts": "^5.2|^6|^7|^8",
        "illuminate/http": "^5.2|^6|^7|^8",
        "illuminate/support": "^5.2|^6|^7|^8",
        "illuminate/routing": "^5.2|^6|^7|^8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "aliases": {
                "Http": "AbcDaConstrucao\\AutorizacaoCliente\\Facades\\Http",
                "JWT": "AbcDaConstrucao\\AutorizacaoCliente\\Facades\\JWT"
            },
            "providers": [
                "AbcDaConstrucao\\AutorizacaoCliente\\Providers\\AutorizacaoServiceProvider"
            ]
        }
    }
}
